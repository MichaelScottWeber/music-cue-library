<% include ../partials/header %>

<div class="container">

  <div class="form">
    <h1>Add New Music</h1>
    <div>
      <form class="" action="/music" method="POST">
        <div class="form-item">
          <input class="" type="text" name="title" placeholder="Song Title">
        </div>
        <div class="form-item">
          <input class="" type="text" name="image" placeholder="Image url">
        </div>
        <div class="form-item">
          <input class="" type="text" name="audio" placeholder="Audio url">
        </div>
        <div class="form-item">
          <textarea class="" type="text" name="description" placeholder="Song Description"></textarea>
        </div>
        <div class="form-item">
          <textarea class="" type="text" name="tags" placeholder="tags"></textarea>
        </div>
        <div class="form-item">
          <button class="">Submit</button>
        </div>
      </form>
      <a href="/music">Go Back</a>
    </div>
  </div>

</div>



<% include ../partials/footer %>
